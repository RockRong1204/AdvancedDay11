<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.rock.event.MainActivity">

    <Button
        android:id="@+id/teach_submit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Hello老司机"/>

    <Button
        android:id="@+id/teach_add_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="添加碎片"/>

    <Button
        android:id="@+id/teach_post_sticky"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="粘性事件"/>
     <Button
        android:id="@+id/teach_jump"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转"/>




    <FrameLayout
        android:id="@+id/teach_container"
        android:layout_width="match_parent"
        android:layout_height="200dp"/>

    <FrameLayout
        android:id="@+id/teach_container_two"
        android:layout_width="match_parent"
        android:layout_height="200dp"/>


</LinearLayout>
